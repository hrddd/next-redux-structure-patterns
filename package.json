{
  "name": "next-redux-structure-patterens",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "lint": "next lint"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "exec": "ts-node --project tsconfig.server.json server/index.ts",
    "ext": "js ts"
  },
  "dependencies": {
    "express": "^4.17.3",
    "next": "12.1.0",
    "nodemon": "^2.0.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "ts-node": "^10.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "17.0.19",
    "@types/react": "17.0.39",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "typescript": "4.5.5"
  }
}
